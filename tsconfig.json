{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/data/*"],
      "@utils/*": ["src/utils/*"],
      "@styles": ["src/styles/*"],
      "@constants": ["src/constants.ts"]
    }
  }
}
